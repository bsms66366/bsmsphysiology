{
  "expo": {
    "name": "bsmsphysiology",
    "slug": "learnPhysiology",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bsmsphysiology",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cjtaylormade.bsmsphysiology",
      "usesAppleSignIn": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.cjtaylormade.bsmsphysiology.refresh"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cjtaylormade.bsmsphysiology"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "exportCompliance": {
      "usesEncryption": false
    },
    "extra": {
      "eas": {
        "projectId": "a2993842-0d28-4704-8291-66ef03edd6ca"
      }
    },
    "owner": "cjtaylormade"
  }
}
